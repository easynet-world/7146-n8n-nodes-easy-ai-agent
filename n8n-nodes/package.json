{
  "name": "n8n-nodes-easy-ai-agent",
  "version": "1.0.0",
  "description": "n8n node for Easy AI Agent - AI-powered task planning and execution with MCP integration",
  "keywords": [
    "n8n-community-node-package",
    "easy-agent",
    "orchestrator",
    "ai-agent",
    "planning",
    "execution",
    "mcp",
    "openrouter",
    "ollama",
    "redis"
  ],
  "license": "MIT",
  "homepage": "https://github.com/boqiangliang/easy-agent-orchestrator",
  "author": {
    "name": "boqiang.liang",
    "email": "boqiang.liang@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boqiangliang/easy-agent-orchestrator.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EasyAgentOrchestratorApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EasyAgentOrchestrator/EasyAgentOrchestrator.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
