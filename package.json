{
  "name": "n8n-nodes-easy-ai-agent",
  "version": "1.0.1",
  "description": "n8n node for Easy AI Agent - AI-powered task planning and execution with MCP integration",
  "keywords": [
    "n8n-community-node-package",
    "easy-agent",
    "orchestrator",
    "ai-agent",
    "planning",
    "execution",
    "mcp",
    "openrouter",
    "ollama",
    "redis"
  ],
  "license": "MIT",
  "homepage": "https://github.com/easynet-world/7146-n8n-nodes-easy-ai-agent#readme",
  "author": {
    "name": "boqiang.liang",
    "email": "boqiang.liang@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easynet-world/7146-n8n-nodes-easy-ai-agent.git"
  },
  "bugs": {
    "url": "https://github.com/easynet-world/7146-n8n-nodes-easy-ai-agent/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s",
    "start": "node src/index.js",
    "demo": "node demo-openrouter.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EasyAgentOrchestratorApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EasyAgentOrchestrator/EasyAgentOrchestrator.node.js"
    ]
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.0",
    "typescript": "^5.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "nodemon": "^3.0.2",
    "semantic-release": "^22.0.0",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-conventionalcommits": "^8.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
